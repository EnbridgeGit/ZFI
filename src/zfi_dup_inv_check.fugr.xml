      <TDID>FU</TDID>
      <TDSPRAS>D</TDSPRAS>
      <TDFORM>S_DOCU_SHOW</TDFORM>
      <TDSTYLE>S_DOCUS1</TDSTYLE>
      <TDVERSION>00008</TDVERSION>
      <TDLINESIZE>072</TDLINESIZE>
     </HEAD>
     <LINES>
      <TLINE>
       <TDFORMAT>/:</TDFORMAT>
       <TDLINE>DEFINE &amp;TEXT&amp;  = &apos;Doppelte Rechnungsprüfung&apos;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>/:</TDFORMAT>
       <TDLINE>DEFINE &amp;EVENT&amp; = &apos;00001110&apos;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>/:</TDFORMAT>
       <TDLINE>DEFINE &amp;TYPE&amp;  = &apos;PROCESS&apos;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>/:</TDFORMAT>
       <TDLINE>INCLUDE FI_OPEN_FI_DOCUMENTATION_1 OBJECT DOKU ID TX</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
      </TLINE>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>PROCESS</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Der Prozess wird immer dann erreicht, wenn der Funktionsbaustein</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>FI_DUPLICATE_INVOICE_CHECK aufgerufen wird.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
      </TLINE>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>SCHNITTSTELLE</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>An die Zusatzkomponente werden prüfungsrelevante Belegdaten übergeben,</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>anhand deren Inhalte entschieden werden kann, ob die Rechnung bereits</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>erfaßt wurde.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>ACHTUNG: Sollten Sie Ihren eigenen Funktionsbaustein durch Kopie des</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>Bausteins FI_DUPLICATE_INVOICE_CHECK angelegt haben, so müssen Sie in</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>der Kopie den Aufruf</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>/</TDFORMAT>
       <TDLINE>CALL FUNCTION &apos;OPEN_FI_PERFORM_00001110_P&apos;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>/</TDFORMAT>
       <TDLINE>entfernen !</TDLINE>
      </TLINE>
     </LINES>
    </item>
